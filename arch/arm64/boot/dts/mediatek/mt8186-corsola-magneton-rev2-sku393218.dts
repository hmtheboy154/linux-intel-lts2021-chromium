/dts-v1/;
#include "mt8186-corsola-steelix.dtsi"

/ {
	model = "Google Magneton sku393218 board";
	compatible = "google,steelix-rev2-sku393218", "google,steelix-rev2-sku393221",
		     "google,steelix-rev2", "google,steelix", "mediatek,mt8186";
};

&gpio_keys {
	status = "disabled";
};

&touchscreen {
	status = "disabled";
};

&usb_c1 {
	status = "disabled";
};
